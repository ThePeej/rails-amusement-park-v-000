<a href="/attractions/new">New Attraction</a>

<ul>
  <% @attractions.each do |attraction| %>
    <li><%= link_to "Go on #{attraction.name}", attraction_path(attraction) if !current_user.admin%></li>
    <li><%= link_to "Show #{attraction.name}", attraction_path(attraction) if !current_user.admin%></li>
  <% end %>
</ul>
