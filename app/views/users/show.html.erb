<% if @message %>
  <div class="center alert">
    <p><%= @message %></p>
  </div>
<% end %>

<h3><%= "ADMIN" if @user.admin %></h3>
<h3>Name: <%= @user.name %></h3>
<h3>Tickets: <%= @user.tickets %></h3>
<h3>Mood: <%= @user.mood %></h3>
<h3>Height: <%= @user.height %></h3>

<%= link_to "Log Out", @user, method: :delete %>
<%= link_to "See attractions", attractions_path %>
